{
  "expo": {
    "name": "fideleSe-mobile",
    "slug": "fideleSe-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "platforms": ["ios", "android"],
    "permissions": ["CAMERA", "AUDIO_RECORDING", "CAMERA_ROLL"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "permissions": ["CAMERA", "AUDIO_RECORDING", "CAMERA_ROLL"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": ["CAMERA", "AUDIO_RECORDING", "CAMERA_ROLL"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "App criado em ReactNative utilizando Expo"
  }
}
